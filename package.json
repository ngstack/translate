{
  "name": "@ngstack/core",
  "description": "An open source toolkit for enterprise Angular applications.",
  "version": "5.0.0",
  "license": "MIT",
  "author": "Denys Vuika <denys.vuika@gmail.com>",
  "scripts": {
    "postinstall": "ngcc",
    "ng": "ng",
    "start": "ng build translate && ng serve",
    "build": "ng build",
    "test": "ng test translate --code-coverage",
    "test:ci": "ng test translate --watch=false --code-coverage",
    "lint": "ng lint --format=stylish",
    "e2e": "ng e2e",
    "spellcheck": "cspell '{src,e2e,projects}/**/*.ts'",
    "build:lib": "ng build translate",
    "build:prod": "ng build translate --prod && cp -r README.md dist/@ngstack/translate",
    "format:check": "prettier --check \"{src,projects}/**/*.{ts,js,css,scss,html}\"",
    "format:fix": "prettier --write \"{src,projects}/**/*.{ts,js,css,scss,html}\"",
    "size": "size-limit"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.1.1",
    "@angular/cdk": "^11.1.1",
    "@angular/common": "^11.1.1",
    "@angular/compiler": "^11.1.1",
    "@angular/core": "^11.1.1",
    "@angular/forms": "^11.1.1",
    "@angular/material": "^11.1.1",
    "@angular/platform-browser": "^11.1.1",
    "@angular/platform-browser-dynamic": "^11.1.1",
    "@angular/router": "^11.1.1",
    "rxjs": "^6.6.3",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.4",
    "@angular/cli": "^11.1.4",
    "@angular/compiler-cli": "^11.1.1",
    "@angular/language-service": "^11.2.0",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.28",
    "codelyzer": "^6.0.0",
    "cspell": "^5.2.4",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "^6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^2.0.1",
    "ng-packagr": "^11.1.4",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "puppeteer": "^7.1.0",
    "size-limit": "^4.9.2",
    "ts-node": "~9.1.1",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "4.1.5"
  },
  "size-limit": [
    {
      "path": "dist/@ngstack/translate/bundles/ngstack-translate.umd.min.js",
      "limit": "5 KB",
      "webpack": false
    }
  ]
}
